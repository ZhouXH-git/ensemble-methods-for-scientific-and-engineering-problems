FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application      simpleFoam;
startFrom        latestTime;
startTime        0;
stopAt           nextWrite;
endTime          1000;
deltaT           0.0005;
writeControl     runTime;
writeInterval    1;
purgeWrite       2;
writeFormat      ascii;
writePrecision   6;
writeCompression off;
timeFormat       fixed;
timePrecision    0;
runTimeModifiable true;

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //